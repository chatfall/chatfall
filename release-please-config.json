{
  "release-type": "node",
  "bump-minor-pre-major": false,
  "bump-patch-for-minor-pre-major": false,
  "draft": false,
  "prerelease": false,
  "include-component-in-tag": false,
  "pull-request-title-pattern": "chore(release): release ${version}",
  "group-pull-request-title-pattern": "chore(release): release ${version}",
  "changelog-path": "CHANGELOG.md",
  "packages": {
    ".": {
      "package-name": "chatfall"
    }
  },
  "changelog-sections": [
    {"type": "feat", "section": "Features", "hidden": false},
    {"type": "fix", "section": "Bug Fixes", "hidden": false},
    {"type": "chore", "section": "Miscellaneous", "hidden": false},
    {"type": "docs", "section": "Miscellaneous", "hidden": false},
    {"type": "perf", "section": "Miscellaneous", "hidden": false},
    {"type": "style", "section": "Miscellaneous", "hidden": false},
    {"type": "refactor", "section": "Miscellaneous", "hidden": false},
    {"type": "build", "section": "Miscellaneous", "hidden": false}
  ],
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
